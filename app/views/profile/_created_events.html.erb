<div class="row" id="profile_created_events"
    ng-controller="createdEventsController" ng-init="init()">

   <div class="col-md-offset-2 col-md-8">
     <h1 ng-if="hasCreatedEvents()">Created Events</h1>

     <ul class="event-list">
       <li class="event-listing"
           ng-repeat="event in getFeedScope().createdEvents
           | orderBy: 'date': true">

        <!-- Event layout -->
        <div class="event_info" role="button"
             data-toggle="modal"
             data-target="#select_participants">

          <%= render "shared/event_body" %>
        </div>

        <!-- Popup layout for participant selection to event -->
        <%= render "event_participants_selection" %>

      </li>
    </ul>

  </div>

</div>
